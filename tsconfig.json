{
    "compilerOptions": {
     "target": "es5",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "module": "commonjs",
      "baseUrl": "./src",
      "paths": {
        "@modules/*": ["modules/*"],
        "@configs/*": ["configs/*"],
        "@main/*": ["main/*"],
        "@errors]/*": ["errors/*"],
        "@utils/*": ["utils/*"]
      },
      "resolveJsonModule": true,
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": false
    }
  }